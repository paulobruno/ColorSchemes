                }
            },
            {
                "name": "Java-string",
                "scope":[
                    "string.quoted.single.java",
                    "string.quoted.double.java"
                ],
                "settings": {
                    "foreground": "#FF00FF",
                    "fontStyle": ""
                }
            },
            {
                "name": "Java-entities",
