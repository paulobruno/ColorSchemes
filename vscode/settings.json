{
    "editor.rulers": [72],
    "editor.renderIndentGuides": true,
    "editor.detectIndentation": false,
    "editor.insertSpaces": true,
    "editor.tabCompletion": false,

    "files.useExperimentalFileWatcher": true,

    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/*.d": true,
        "**/*.o": true,

    },

    "workbench.colorCustomizations": {
        // All brackgrounds dark grey
        "statusBar.background": "#111111",
        "activityBar.background": "#111111",
        "panel.background": "#111111",
        "sideBar.background": "#111111",
        "dropdown.background": "#111111",
        "editorGroup.background": "#111111",
        "titleBar.activeBackground": "#111111",
        "tab.activeBackground": "#000000",
        "tab.inactiveBackground": "#111111",
        "tab.hoverBackground": "#222222",
        "editorGroupHeader.tabsBackground": "#222222",
        "input.background": "#222222",
        "statusBarItem.hoverBackground": "#333333",
        "scrollbarSlider.hoverBackground": "#444444",

        "list.hoverBackground": "#444444",
        "list.activeSelectionBackground": "#333333",
        "list.focusBackground": "#444444",
        "list.inactiveSelectionBackground": "#333333",
                
        // terminal settings
        "terminal.background": "#220022",
        "terminal.foreground": "#FFFFFF",
        "terminal.ansiBrightGreen": "#99DD00",
        "terminal.ansiBrightBlue": "#66a6d1",
        "terminal.ansiBrightMagenta": "#BB55BB",

        // editor settings
        "editor.background": "#000000",
        "editor.foreground": "#ffffff",
        "editorBracketMatch.background": "#000080",
        "editorBracketMatch.border": "#000080",
        "editorRuler.foreground": "#222222",
        "editorIndentGuide.background": "#111111",
        "editorIndentGuide.activeBackground": "#222222",
        "editor.selectionHighlightBorder": "#666666",
        "editorHoverWidget.background": "#222222",
        "editor.hoverHighlightBackground": "#222222"
    },
    
    "editor.tokenColorCustomizations": {
        "textMateRules": [

            // JSON Rules
            {
                "name": "Json-PropertyName",
                "scope":"support.type.property-name",
                "settings": {
                    "foreground": "#FF0000",
                    "fontStyle": "bold"
                }
            },
            {
                "name": "Json-PropertyContent",
                "scope":[
                    "string.quoted.double",
                    "constant.language.json"
                ],
                "settings": {
                    "foreground": "#FFFFFF",
                    "fontStyle": ""
                }
            },

            // C Rules
            {
                "name": "C-Preprocessor",
                "scope":[
                    "meta.preprocessor",
                    "meta.preprocessor constant.numeric",
                    "meta.preprocessor constant.character.escape",
                    "meta.preprocessor keyword.operator",
                    "meta.preprocessor keyword.operator.sizeof",
                    "meta.preprocessor keyword.control",
                    "meta.preprocessor entity.name.function",
                    "meta.preprocessor keyword.control.directive",
                    "meta.preprocessor string.quoted",
                    "meta.preprocessor string.quoted.double",
                    "meta.preprocessor string.quoted.double constant.character.escape",
                    "meta.preprocessor support.function",
                    "meta.preprocessor variable.parameter.preprocessor",
                    "meta.preprocessor variable.other",
                    "meta.preprocessor meta.function"
                ],
                "settings": {
                    "foreground": "#00FF00",
                    "fontStyle": ""
                }
            },
            {
                "name": "C-CompilerDirective",
                "scope":[
                    "keyword.control.directive.pragma",
                    "entity.other.attribute-name.pragma"
                ],
                "settings": {
                    "foreground": "#FFa500",
                    "fontStyle": ""
                }
            },
            {
                "name": "C-String",
                "scope":[
                    "meta.block string.quoted.double",
                    "string.quoted.double constant.character.escape",
                    "string.quoted.single",
                    "constant.character.escape"
                ],
                "settings": {
                    "foreground": "#FF00FF",
                    "fontStyle": ""
                }
            },
            {
                "name": "C-Keyword",
                "scope": [
                    "keyword.control",
                    "storage.type",
                    "support.type",
                    "storage.modifier",
                    "variable.language",
                    "keyword.operator.cast",
                    "keyword.operator.sizeof"
                ],
                "settings": {
                    "foreground": "#FF0000",
                    "fontStyle": "bold"
                }
            },
            {
                "name": "C-Operator",
                "scope": "keyword.operator",
                "settings": {
                    "foreground": "#FFFFFF",
                    "fontStyle": ""
                }
            },
            {
                "name": "C-Number",
                "scope":"constant.numeric",
                "settings": {
                    "foreground": "#FFA500",
                    "fontStyle": ""
                }
            },
            {
                "name": "C-Type",
                "scope":"entity.name.type",
                "settings": {
                    "foreground": "#FFFFFF",
                    "fontStyle": ""
                }
            },
            {
                "name": "C-Comments",
                "scope":"comment",
                "settings": {
                    "foreground": "#8BC0F3",
                    "fontStyle": "italic"
                }
            },
            {
                "name": "C-Function",
                "scope": [
                    "meta.function",
                    "meta.function entity.name.function",
                    "entity.name.function",
                    "support.function"
                ],
                "settings": {
                    "foreground": "#FFFFFF",
                    "fontStyle": ""
                }
            },
            {
                "name": "C-Constant",
                "scope": "constant.language.c",
                "settings": {
                    "foreground": "#FFA500",
                    "fontStyle": ""
                }
            },
            {
                "name": "C-Variable",
                "scope":[
                    "variable",
                    "support.type.posix-reserved.c"
                ],
                "settings": {
                    "foreground": "#FFFFFF",
                    "fontStyle": ""
                }
            },

            // SHELL Rules
            {
                "name": "Sh-Variable",
                "scope":[
                    "string.other.math.shell",
                    "string.interpolated.dollar.shell",
                    "variable.other.normal.shell",
                    "string.quoted.double.shell",
                    "variable.other.bracket.shell"
                ],
                "settings": {
                    "foreground": "#FF00FF",
                    "fontStyle": ""
                }
            },
            {
                "name": "Sh-Backticks",
                "scope":[
                    "string.interpolated.backtick.shell",
                    "variable.other.loop.shell",
                ],
                "settings": {
                    "foreground": "#FFFF00",
                    "fontStyle": ""
                }
            },
            {
                "name": "Sh-Definition",
                "scope":[
                    "punctuation.definition.comment.shebang.shell",
                    "comment.line.number-sign.shebang.shell"
                ],
                "settings": {
                    "foreground": "#00FF00"
                }
            },
            {
                "name": "Sh-Function",
                "scope":[
                    "support.function.builtin.shell",
                    "variable.other.positional"
                ],
                "settings": {
                    "foreground": "#FF0000",
                    "fontStyle": "bold"
                }
            },
            
            // PYTHON Rules
            {
                "name": "Python-comment",
                "scope":[
                    "punctuation.definition.comment.python",
                    "comment.line.number-sign.python"
                ],
                "settings": {
                    "foreground": "#8BC0F3",
                    "fontStyle": "italic"
                }
            },
            {
                "name": "Python-doc-comment",
                "scope":[
                    "string.quoted.docstring.multi.python"
                ],
                "settings": {
                    "foreground": "#8BC0F3",
                    "fontStyle": ""
                }
            },
            {
                "name": "Python-string",
                "scope":[
                    "string.quoted.single.python",
                    "constant.character.escape.python",
                    "constant.character.format.placeholder.other.python"
                ],
                "settings": {
                    "foreground": "#FF00FF",
                    "fontStyle": ""
                }
            },
            {
                "name": "Python-internal",
                "scope":[
                    "keyword.control.flow.python",
                    "support.variable.magic.python",
                    "keyword.operator.logical.python",
                    "support.type.python",
                    "variable.legacy.builtin.python",
                    "support.function.builtin.python"
                ],
                "settings": {
                    "foreground": "#FF0000",
                    "fontStyle": "bold"
                }
            },
            {
                "name": "Python-import",
                "scope":[
                    "keyword.control.import.python",
                ],
                "settings": {
                    "foreground": "#FF0000",
                    "fontStyle": ""
                }
            },
            {
                "name": "Python-function",
                "scope":[
                    "entity.name.function.python",
                    "support.function.magic.python",
                    "storage.type.function.python"
                ],
                "settings": {
                    "foreground": "#00FF00",
                    "fontStyle": ""
                }
            },
            {
                "name": "Python-class",
                "scope":[
                    "entity.name.type.class.python",
                    "storage.type.class.python"
                ],
                "settings": {
                    "foreground": "#00FF00",
                    "fontStyle": "italic"
                }
            },
            {
                "name": "Python-text-other",
                "scope":[
                    "invalid.deprecated.semicolon.python"
                ],
                "settings": {
                    "foreground": "#FFFFFF",
                    "fontStyle": ""
                }
            },
            {
                "name": "Python-constant",
                "scope":[
                    "constant.language.python"
                ],
                "settings": {
                    "foreground": "#FFFFFF",
                    "fontStyle": "italic"
                }
            },

            // JAVA Rules
            {
                "name": "Java-import",
                "scope":[
                    "keyword.other.package.java",
                    "keyword.other.import.java"
                ],
                "settings": {
                    "foreground": "#00FF00",
                    "fontStyle": ""
                }
            },
            {
                "name": "Java-keyword",
                "scope":[
                    "keyword.control.new.java",
                    "storage.type.primitive.java",
                    "storage.type.primitive.array.java",
                    "punctuation.section.class.begin.bracket.curly.java",
                    "punctuation.section.class.end.bracket.curly.java",
                    "punctuation.section.method.begin.bracket.curly.java",
                    "punctuation.section.method.end.bracket.curly.java",
                    "punctuation.section.block.begin.bracket.curly.java",
                    "punctuation.section.block.end.bracket.curly.java",
                    "keyword.operator.instanceof.java"
                ],
                "settings": {
                    "foreground": "#FF0000",
                    "fontStyle": "bold"
                }
            },
            {
                "name": "Java-custom-types",
                "scope":[
                    "storage.type.java",
                    "storage.type.object.array.java",
                    "storage.type.generic.java",
                    "entity.other.inherited-class.java"
                ],
                "settings": {
                    "foreground": "#ff0000",
                    "fontStyle": ""
                }
            },
            {
                "name": "Java-text-other",
                "scope":[
                    "keyword.control.ternary.java",
                    "storage.type.function.arrow.java",
                    "punctuation.section.method.begin.bracket.curly.java",
                    "punctuation.section.method.end.bracket.curly.java",
                    "punctuation.section.block.begin.bracket.curly.java",
                    "punctuation.section.block.end.bracket.curly.java",
                    "punctuation.section.class.begin.bracket.curly.java",
                    "punctuation.section.class.end.bracket.curly.java",
                    "punctuation.bracket.angle.java"
                ],
                "settings": {
                    "foreground": "#FFFFFF",
                    "fontStyle": ""
                }
            },
            {
                "name": "Java-constant",
                "scope":[
                    "constant.language.java"
                ],
                "settings": {
                    "foreground": "#FFFFFF",
                    "fontStyle": "bold"
                }
            },
            {
                "name": "Java-string",
                "scope":[
                    "string.quoted.single.java",
                    "string.quoted.double.java"
                ],
                "settings": {
                    "foreground": "#FF00FF",
                    "fontStyle": ""
                }
            },
            {
                "name": "Java-entities",
                "scope": [
                    "storage.type.annotation.java"
                ],
                "settings": {
                    "foreground": "#FF0000",
                    "fontStyle": "italic"
                }
            }
        ]
    },
    "workbench.activityBar.visible": true,
    "workbench.statusBar.visible": true,
    "editor.renderWhitespace": "none",
    "workbench.sideBar.location": "left",
    "window.menuBarVisibility": "toggle",
    "workbench.statusBar.feedback.visible": false,
    "editor.minimap.enabled": false,
    "window.zoomLevel": 0,
    "explorer.confirmDragAndDrop": false,
    "workbench.startupEditor": "newUntitledFile",
    "terminal.integrated.rendererType": "dom",
    "python.pythonPath": "python3.6",

    "doxdocgen.file.versionTag": "@version 1.0",
    "doxdocgen.generic.authorEmail": "gitlab.com/paulobruno",
    "doxdocgen.generic.authorName": "Paulo Bruno Serafim",
    "doxdocgen.generic.dateFormat": "YYYY-MM-DD",
    "doxdocgen.file.fileOrder": [
        "file",
        "author",
        "empty",
        "brief",
        "empty",
        "version",
        "date",
        "empty",
        "copyright",
        "custom"
    ]
}
